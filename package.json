{
  "name": "eastendtradesguild",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "airtable": "^0.7.1",
    "axios": "^0.19.0",
    "dotenv": "^6.1.0",
    "leaflet": "^1.3.4",
    "leaflet.markercluster": "^1.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-leaflet": "^2.1.0",
    "react-leaflet-markercluster": "^2.0.0-rc3",
    "react-scripts": "^3.1.1",
    "styled-components": "^3.4.10",
    "tachyons": "^4.11.1",
    "tachyons-cli": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm run css && npm run start",
    "build": "run-p build:**",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda",
    "test": "react-scripts test",
    "css": "tachyons src/css/app.css > src/components/tachyons.css --minify",
    "test:coverage": "npm test -- --coverage",
    "coverage": "codecov",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "canvas": "^1.6.13",
    "codecov": "^3.1.0",
    "concurrently": "^4.0.1",
    "eslint-config-prettier": "^3.1.0",
    "lodash": "^4.17.15",
    "merge": ">=1.2.1",
    "netlify-lambda": "^1.6.3",
    "nock": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.3",
    "react-testing-library": "^5.2.0",
    "supertest": "^3.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/**/serviceWorker.js",
      "!**/coverage/**",
      "!**/server.js",
      "!**/src/index.js"
    ]
  }
}
