{
  "name": "eastendtradesguild",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9000",
  "dependencies": {
    "@testing-library/react": "^9.1.4",
    "airtable": "^0.7.1",
    "antd": "^3.23.3",
    "axios": "^0.19.0",
    "install": "^0.13.0",
    "leaflet": "^1.3.4",
    "leaflet.markercluster": "^1.4.1",
    "npm": "^6.11.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-leaflet": "^2.1.0",
    "react-leaflet-markercluster": "^2.0.0-rc3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "wait:lambda": "wait-on http://localhost:9000/.netlify/functions/getLocations",
    "dev-server": "react-scripts start",
    "start": "run-p start:**",
    "start:lambda": "netlify-lambda serve src/lambda",
    "start:client": "run-s wait:lambda dev-server",
    "build": "run-p build:**",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda",
    "test": "react-scripts test",
    "css": "tachyons src/css/app.css > src/components/tachyons.css --minify",
    "test:coverage": "npm test -- --coverage",
    "coverage": "codecov"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "canvas": "^1.6.13",
    "codecov": "^3.5.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/**/serviceWorker.js",
      "!**/coverage/**",
      "!**/server.js",
      "!**/src/index.js"
    ]
  }
}
